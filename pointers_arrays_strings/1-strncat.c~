#include "main.h"

/**
 **_strncat - concatenates two strings
 *@dest: first string
 *@src: second string
 *@n: variable of strncat
 *Return: success
 */

char *_strncat(char *dest, char *src, int n)
{
int i = 0;
int j = 0;
while (i < n && dest[i] != '\0')
i++;
while (j < n && src[j] != '\0')
{
dest[i] = src[j];
j++;
i++;
}
return (dest);
}
