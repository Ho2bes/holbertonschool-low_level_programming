#include "main.h"

/**
*print_diagonal - Compute the absolute value of an integer
*@n : - variable
* Return: The absolute value
*/

void print_diagonal(int n)
int main(void)
{ 
    char a = '\';
    int n = 5;

    for(int i = 0; i < size; i++)
    {
        for(int j = 0; j < n; j++)
        {
            if(j == i)
                _putchar('a');
            else
                _putchar(' ');
        }
        printf("\n");
    }

    return 0;
}
